@article{Henze1986,
	Abstract = {In a recent paper Azzalini (1985) introduced the class $\scr{SN}=\{\scr{SN}(\lambda)\colon \lambda \in {\Bbb R}\}$ of skew-normal probability distributions and studied its main properties. The salient features of the class 𝒮𝒩 are mathematical tractability and strict inclusion of the Normal law (for λ=0). The shape parameter λ, to some extent, controls the index of skewness. It is the purpose of this note to give a probabilistic representation of the distribution 𝒮𝒩(λ) in terms of Normal and truncated Normal laws. This representation reveals the "structure" of the class 𝒮𝒩 and indicates the kind of departure from normality. The moments of a random variable $Z_{\lambda}$ with the distribution 𝒮𝒩(λ) are explicitly determined, and an efficient method for the Monte Carlo generation of $Z_{\lambda}$ is shown.},
	Author = {Henze, Norbert},
	Copyright = {Copyright {\copyright} 1986 Board of the Foundation of the Scandinavian Journal of Statistics},
